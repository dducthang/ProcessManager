<Window x:Class="ProcessesManager.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessesManager"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800"
        >
    <Canvas>
        <TextBox Name="PwTextBox" Width="200" Height="50" Canvas.Left="300" Canvas.Top="192"/>
        <Button Name="Login" Content="Login" Height="40" Width="100" Canvas.Left="350" Canvas.Top="270" IsDefault="True" Click="Login_Click"/>
    </Canvas>
</Window>
