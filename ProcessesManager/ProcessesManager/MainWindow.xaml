<Window x:Class="ProcessesManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessesManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button Name="StartBtn" Content="Start" Canvas.Left="159" Canvas.Top="256" Height="49" Width="151" Click="StartBtn_Click"/>
        <TextBox Name="PassTextBlock"  Height="29" Width="200" Canvas.Left="159" Canvas.Top="188" KeyDown="tb_KeyDown" FontSize="14"/>
        <Label Content="enter password" Canvas.Left="159" Canvas.Top="142" FontWeight="Bold" FontSize="16"/>
        <Label x:Name="lb_timer" Content="timer" Canvas.Left="400" Canvas.Top="145" FontStyle="Italic" FontSize="14" RenderTransformOrigin="-0.215,0.77"/>
        <Label x:Name="time_left" Content="timer" Canvas.Left="400" Canvas.Top="276" FontStyle="Italic" FontSize="14"/>
        <Label x:Name="noti" Content="Notification :" Canvas.Left="400" Canvas.Top="226" RenderTransformOrigin="-0.526,1.042" FontSize="14" FontWeight="Bold"/>
    </Canvas>
</Window>
