        <Label x:Name="noti" Content="enter password" Canvas.Left="300" Canvas.Top="121" FontWeight="Bold" FontSize="16"/>
        <Label x:Name="lb_timer" Content="timer" Canvas.Left="506" Canvas.Top="124" FontStyle="Italic" FontSize="14"/>
